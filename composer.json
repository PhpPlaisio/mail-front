{
  "name": "plaisio/mail-front",
  "description": "Plaisio: Mail - Front",
  "keywords": [
    "Plaisio",
    "Mail Message"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-mbstring": "*",
    "plaisio/company-resolver": "^2.2.0",
    "plaisio/kernel": "^3.3.0",
    "plaisio/mail-message": "^2.2.0",
    "setbased/exception": "^2.6.0",
    "setbased/php-stratum-mysql": "^7.1.1"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "plaisio/console-kernel": "^1.2.0",
    "setbased/php-stratum": "^6.5.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Mail\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
