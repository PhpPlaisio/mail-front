{
  "name": "plaisio/mail-front",
  "description": "Plaisio: Mail - Front",
  "keywords": [
    "Plaisio",
    "Mail Message"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-mbstring": "*",
    "plaisio/company-resolver": "^2.1.0",
    "plaisio/kernel": "^3.2.1",
    "plaisio/mail-message": "^2.1.0",
    "setbased/exception": "^2.3.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "plaisio/console-kernel": "^1.0.1",
    "setbased/php-stratum": "^6.3.1",
    "setbased/php-stratum-mysql": "^6.6.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Mail\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
